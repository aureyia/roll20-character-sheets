include ../oracle/oracleRollTemplate

rolltemplate.sheet-rolltemplate-starforged_oracles
  .sheet-roll-container
    .sheet-subheader {{header}}
    | {{#custom-oracle}}
    .sheet-result-row
      .sheet-oracle-table-text {{custom-oracle}}
    | {{/custom-oracle}}

    each category in oracles
      each oracle in category.Oracles
        +oracleRollTemplate(oracle)
      if category.Subcategories
        each subCategory in category.Subcategories
          +oracleRollTemplate(subCategory)